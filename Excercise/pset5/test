def shorten(word):
    vowels = "aeiouAEIOU"
    shortened_word = ""
    for char in word:
        if char not in vowels:
            shortened_word += char
        else:
            print(
                f"Removing vowel: {char}"
            )  # Debugging line to see what's being removed
    print(
        f"Final shortened word: {shortened_word}"
    )  # Debugging line to see the result after loop
    return shortened_word
